{
	"tasks": [
		{
			"type": "shell",
			"label": "SDL2",
			"command": "/usr/bin/clang",
			"args": [
				"-g",
				"foodhunter.c",
				"-o",
				"main.exe",
				"-I/opt/homebrew/include",
				"-L/opt/homebrew/lib",
				"-lSDL2main",
				"-lSDL2",
				"-lSDL2_image",
				"-lSDL2_mixer",
				"-lSDL2_ttf"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
	],
	"version": "2.0.0"
}