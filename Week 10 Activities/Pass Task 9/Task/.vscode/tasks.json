{
    "tasks": [
		{
			"type": "shell",
			"label": "music",
			"command": "gcc",
			"args": [
				"-g",
				"gui_music_player.c",
				"input_functions.c",
				"-o",
				"main.exe",
				"-IC:\\msys64\\mingw64\\include",
				"-LC:\\msys64\\mingw64\\lib",
                "-lmingw32",
				"-lSDL2main",
				"-lSDL2",
				"-lSDL2_image",
				"-lSDL2_mixer",
				"-lSDL2_ttf"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}
    ],
    "version": "2.0.0"
}